{
  "name": "express-ts-prisma",
  "version": "1.0.0",
  "description": "Belajar membangun rest api menggunakan express berbasis typescript dan prisma",
  "keywords": [
    "restfull",
    "api",
    "express",
    "typescript",
    "prisma"
  ],
  "license": "MIT",
  "author": "Kaza Desky",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "build": "tsc --build",
    "format": "prettier . --write",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier . --check",
    "check:lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "3.5.2",
    "prisma": "^6.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
